{
  "name": "web-dev-boilerplate_parcel",
  "version": "1.0.0",
  "description": "Parcel を使ったウェブサイト開発環境のボイラープレート",
  "main": "index.html",
  "browserslist": [
    "last 2 versions",
    "ie > 9",
    "Android > 3",
    "iOS > 6"
  ],
  "scripts": {
    "start": "parcel src/index.pug --open --out-dir .tmp",
    "build": "parcel build src/index.pug --no-source-maps --public-url ./",
    "prettier:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oti/web-dev-boilerplate_parcel.git"
  },
  "author": "oti (https://twitter.com/otiext)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oti/web-dev-boilerplate_parcel/issues"
  },
  "homepage": "https://github.com/oti/web-dev-boilerplate_parcel#readme",
  "dependencies": {
    "core-js": "^3.6.5"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "pug": "^2.0.4",
    "sass": "^1.26.3",
    "typescript": "^3.8.3"
  }
}
